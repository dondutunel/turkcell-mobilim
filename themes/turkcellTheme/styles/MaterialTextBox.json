{
    ".materialTextBox": {
        "ellipsizeMode": "END",
        "textAlignment": "MIDLEFT",
        "textColor": "rgba( 0,0,0,1 )",
        "errorColor": "rgba( 255,0,0,1 )",
        "hintTextColor": "rgba( 255,255,255,0.7 )",
        "cursorColor": "rgba( 255,203,8,1 )",
        "selectedHintTextColor": "rgba( 255,203,8,1 )",
        "lineColor": {
            "normal": "rgba( 255,255,255,1 )",
            "selected": "rgba( 255,203,8,1 )"
        },
        "font": {
            "size": 17,
            "family": "SFProText",
            "style": "Medium"
        },
        "&-labelsFont": {
            "font": {
                "size": 14,
                "family": "Default"
            }
        },
        "&-hintFont": {
            "font": {
                "size": 13,
                "family": "SFProText",
                "style": "Regular"
            }
        },
        "&-rightLayout": {
            "marginLeft": 1,
            "marginRight": 1,
            "paddingBottom": 4,
            "flexProps": {
                "alignItems": "FLEX_END",
                "justifyContent": "FLEX_END",
                "flexGrow": 1
            },
            "+Device:Device.os === \"Android\"": {
                "marginLeft": 4,
                "marginRight": 4
            }
        },
        "&-rightLabel": {
            "textColor": "rgba( 119,119,119,1 )",
            "textAlignment": "MIDRIGHT",
            "font": {
                "size": 12,
                "family": "Default"
            }
        },
        "&-wrapper": {
            "height": 48,
            "width": null,
            "marginLeft": -1,
            "marginRight": -1,
            "marginBottom": 16,
            "+Device:Device.os === \"Android\"": {
                "height": 81,
                "marginBottom": -8,
                "marginLeft": -4,
                "marginRight": -4
            },
            "&-dropArrow": {
                "flexProps": {
                    "positionType": "ABSOLUTE"
                },
                "right": 2,
                "height": 30,
                "width": 30,
                "top": 20,
                "image": "arrowbottom.png",
                ".hidden": {
                    "visible": false
                }
            }
        }
    }
}
