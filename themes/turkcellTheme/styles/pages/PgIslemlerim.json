{
	"#pgIslemlerim": {
		"backgroundColor": {
			"startColor": "rgba(56,191,242,1)",
			"endColor": "rgba(41,152,227,1)",
			"direction": "VERTICAL"
		},
		"flexProps": {
			"alignItems": "STRETCH"
		},
		"paddingTop": 8,
		"&-title": {
			"font": {
				"size": 28,
				"family": "SFProText",
				"style": "Semibold",
				"bold": true,
				"italic": false
			},
			"height": 41,
			"textColor": "rgba(255,255,255,1)",
			"marginTop": 8,
			"marginLeft": 15
		},
		"&-mainItems": {
			"height": 130,
			"flexProps": {
				"flexDirection": "ROW",
				"alignItems": "CENTER"
			}
		},
		"&-subMenu": {
			"flexProps": {
				"flexGrow": 1
			},
			"height": null,
			"backgroundColor": "rgba(255,255,255,1)",
			"borderRadius": 20,
			"marginTop": 10,
			"paddingLeft": 15,
			"paddingRight": 15,
			"&-switch": {
				".invisible": {
					"maxHeight": 0,
					"marginBottom": 0
				},
				"marginTop": 22
			},
			"&-flMain": {
				"flexProps": {
					"alignItems": "CENTER",
					"flexDirection": "COLUMN",
					"flexGrow": 1,
					"justifyContent": "CENTER"
				},
				"height": null
			}
		},
		"paddingBottom": 0,
		"&-fakeLayout": {
			"bottom": 0,
			"left": 0,
			"right": 0,
			"backgroundColor": "rgba( 255, 255, 255, 1 )",
			"height": 50,
			"flexProps": {
				"positionType": "ABSOLUTE"
			}
		}
	}
}